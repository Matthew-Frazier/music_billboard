<h1>Artists</h1>
  <%= link_to "Create Artist", new_artist_path %>
<% @artists.each do |artist| %>
  <h3> <%= artist.name %> </h3>
  <%= link_to "Show Artist", artist_path(artist) %>
  <%= link_to "Edit Artist", edit_artist_path(artist) %>
  <%= link_to "Delete Artist", artist, method: :delete %>
<% end %>

